{
    "manifest_version": 2,
    
    "name": "My First Chrome Extension",
    "version": "0.1.0.0",
    "description": "Computer Science Chrome Extension project to Modify HTML and CSS",
    
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["content.js"],
        "matches": ["<all_urls>"]
    }],
    "browser_action": {
        "default_icon": "hpicon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}
